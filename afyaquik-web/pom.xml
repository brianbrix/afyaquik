        </dependency>
        <dependency>
            <groupId>com.afyaquik</groupId>
            <artifactId>afyaquik-users</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.afyaquik</groupId>
            <artifactId>dtos</artifactId>
            <version>0.0.1-SNAPSHOT</version>
            <scope>compile</scope>
        </dependency>
